<%- cache(legal) do %>
  <%= element_view_for(legal) do |el| %>
    <div class="impressum">
      <%= el.render :impressum %>
    </div>
    <div class="datenschutz">
      <%= el.render :datenschutz %>
    </div>
    <div class="copyright">
      <%= el.render :copyright %>
    </div>
  <% end %>
<% end %>
