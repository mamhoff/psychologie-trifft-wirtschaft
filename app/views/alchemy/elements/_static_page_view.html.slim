- cache(static_page_view) do
  = element_view_for(static_page_view) do |el|
    .top-background
      = el.render :image, {size: "1300x400"}, class: "img-responsive head-image"
      .container
        .row.relative
          .col-sm-8.col-sm-offset-4
            .inside-image
              h1= el.render :super_title
    .container.body
      .row
        .col-sm-8.col-sm-offset-4
          h3= el.render :title
          = el.render :teaser
          = el.render :body
